<package>
  <name>mcr_global_planner</name>
  <version>1.0.0</version>
  <description>
     Collection of b-it-bots global planners.
  </description>
  <maintainer email="torsten.jandt@smail.inf.h-brs.de">Torsten Jandt</maintainer>
